<launch>
    <!--- Sim Time -->
    <param name="/use_sim_time" value="true"/>

<!--    <node name="rvizvisualisation" pkg="rviz" type="rviz" output="log"-->
<!--          args="-d $(find msckf_viw)/launch/vins_rviz_config.rviz"/>-->

    <node name="RunKAISTData" pkg="msckf_viw" type="RunKAISTData" output="screen"
          args=" $(find msckf_viw)/params/KAIST.yaml">
        <param name="project_dir" type="str" value="$(find msckf_viw)/"/>
    </node>

    <!--    按空格播放-->
    <arg name="bag" default=" /media/qzj/Dataset/slamDataSet/KAIST/urban28_4.bag"/>
    <node pkg="rosbag" type="play" output="log" name="rosbag" args="--pause --clock $(arg bag) -r 1 -s 0"/>

</launch>


